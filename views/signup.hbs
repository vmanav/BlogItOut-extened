            height: 80%;
            position: relative;
        }

        .trendingBlogsContainer {
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .trendingCard {
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
            background-color: white;
            height: 100%;
            width: 90%;
            border-radius: 7px;
            overflow: hidden;
            text-align: justify;
        }
    </style>

